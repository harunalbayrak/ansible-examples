Listen 9757

<VirtualHost *:9757>
    WSGIDaemonProcess safirgocmen-api processes=2 threads=10 user=stack display-name=%{GROUP}
    WSGIProcessGroup safirgocmen-api
    WSGIScriptAlias / /var/www/safir_gocmen/app.wsgi
    WSGIApplicationGroup %{GLOBAL}
    <IfVersion >= 2.4>
        ErrorLogFormat "%{cu}t %M"
    </IfVersion>
    ErrorLog /var/log/apache2/safir_gocmen_error.log
    CustomLog /var/log/apache2/safir_gocmen_access.log combined
    LogLevel info
</VirtualHost>

WSGISocketPrefix /var/run/apache2
